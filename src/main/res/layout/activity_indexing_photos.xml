<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.aleksandra.combined.IndexingPhotosActivity">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="150dp"
        android:scaleType="centerCrop"
        android:layout_height="92dp"
        android:layout_marginTop="25dp"
        android:onClick="photoOnClick"
        android:layout_constraintLeft_toLeftOf="parent"
        android:layout_constraintRight_toLeftOf="@+id/imageView2"
        android:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/pic_2017_04_29_17_10_42" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="93dp"
        android:layout_marginTop="38dp"
        android:layout_constraintLeft_toLeftOf="parent"
        android:layout_constraintRight_toLeftOf="@+id/imageView3"
        android:layout_constraintTop_toBottomOf="@+id/imageView5"
        android:src="@drawable/alaska_67307_960_720" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="93dp"
        android:layout_marginTop="38dp"
        android:layout_constraintLeft_toRightOf="@+id/imageView"
        android:layout_constraintRight_toRightOf="parent"
        android:layout_constraintTop_toBottomOf="@+id/imageView4"
        android:src="@drawable/alaska_566722_960_720" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="150dp"
        android:layout_height="93dp"
        android:layout_marginTop="39dp"
        android:layout_constraintLeft_toRightOf="@+id/imageView"
        android:layout_constraintRight_toRightOf="parent"
        android:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:src="@drawable/blue_sky_1842136_960_720" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="150dp"
        android:layout_height="93dp"
        android:layout_marginTop="40dp"
        android:layout_constraintHorizontal_bias="0.51"
        android:layout_constraintLeft_toLeftOf="parent"
        android:layout_constraintRight_toLeftOf="@+id/imageView3"
        android:layout_constraintTop_toBottomOf="@+id/imageButton"
        android:src="@drawable/masuria_787895_960_720" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="150dp"
        android:layout_height="93dp"
        android:layout_constraintLeft_toRightOf="@+id/imageButton"
        android:layout_constraintRight_toRightOf="parent"
        android:layout_constraintTop_toTopOf="@+id/imageButton"
        android:src="@drawable/oaks_995362_960_720" />
</RelativeLayout>
